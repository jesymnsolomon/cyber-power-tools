{% # theme-check-disable %}
{% comment %} custom fonts {% endcomment %}
<link
  rel="preload"
  href="https://cdn.jsdelivr.net/gh/jesymnsolomon/cyber-power-tools@a8c916c73b8db56ec9e9a0df5ec8beb32d00b1c5/assets/Messapia-Bold.otf"
  as="font"
  type="font/otf"
  crossorigin
>

{%- unless settings.type_body_font.system? -%}
  <link
    rel="preload"
    as="font"
    href="{{ settings.type_body_font | font_url }}"
    type="font/woff2"
    crossorigin
    fetchpriority="low"
  >
{%- endunless -%}
{%- unless settings.type_subheading_font.system? -%}
  <link
    rel="preload"
    as="font"
    href="{{ settings.type_subheading_font | font_url }}"
    type="font/woff2"
    crossorigin
    fetchpriority="low"
  >
{%- endunless -%}
{%- unless settings.type_heading_font.system? -%}
  <link
    rel="preload"
    as="font"
    href="{{ settings.type_heading_font | font_url }}"
    type="font/woff2"
    crossorigin
    fetchpriority="low"
  >
{%- endunless -%}
{%- unless settings.type_accent_font.system? -%}
  <link
    rel="preload"
    as="font"
    href="{{ settings.type_accent_font | font_url }}"
    type="font/woff2"
    crossorigin
    fetchpriority="low"
  >
{%- endunless -%}
{% # theme-check-enable %}
